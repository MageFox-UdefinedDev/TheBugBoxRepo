<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BugBox.Pages.TrapDetail"
             Title="{Binding SingleTrap.Name}">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="{Binding SingleTrap.Name}" />
            <Label Text="{Binding SingleTrap.TrapId}" />
            <StackLayout x:Name="MapStackLayout" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>